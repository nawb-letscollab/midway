{
  "name": "@midwayjs/prometheus",
  "version": "3.1.0",
  "description": "midway component for prometheus",
  "main": "src/index.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "typings": "index.d.ts",
    "main": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "node --require=ts-node/register ../../node_modules/.bin/jest --runInBand",
    "cov": "node --require=ts-node/register ../../node_modules/.bin/jest --runInBand --coverage --forceExit",
    "ci": "npm run cov --ts"
  },
  "keywords": [
    "midway",
    "IoC",
    "plugin",
    "prometheus"
  ],
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "index.d.ts"
  ],
  "license": "MIT",
  "devDependencies": {
    "@midwayjs/core": "workspace:^",
    "@midwayjs/decorator": "workspace:^",
    "@midwayjs/koa": "workspace:^",
    "@midwayjs/mock": "workspace:^",
    "@types/request": "2.48.8"
  },
  "dependencies": {
    "prom-client": "^14.0.0",
    "request": "2.88.2"
  },
  "engines": {
    "node": ">=12"
  }
}
